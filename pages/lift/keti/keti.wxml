</view>
</view>
</view>
  <!-- 速度 -->
  <view  class="section_keti_dl">
<text>速度:</text>
<!-- 下拉框 -->
<view  class='dl_select_box'>
<view  class='dl_select' catchtap="v_selectTaps">
<text class='dl_select_text' >{{V[V_index]}}</text>
</view>
<view class='dl_option_box' style='height:{{V_shows?(V.length>5?300:DL.length*60):0}}rpx;' >
<text class='dl_option' style='{{V_index==V.length-1&&"border:0;"}}' wx:for="{{V}}" wx:key="this" data-vindex1='{{index}}'  catchtap ='v_optionTaps'>{{item}}</text>
</view>
</view>
</view>
  <!-- 层站 -->
  <view  class="section_keti_dl">
<text>层站: </text>
