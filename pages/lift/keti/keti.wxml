<view class="section_keti_guantong">
<text>梯型:</text>
  <!-- 下拉框 -->
  <view class='guantong_select_box'>
      <view class='guantong_select' catchtap='model_s_selectTaps'>
        <text class='guantong_select_text'>{{MODEL_S[MODEL_S_index]}}</text>
        <!-- <image class='words_img' src='../../images/sanjiao.png'></image> -->
         <!-- 下拉框 图片  <image class='select_img {{shows&&"select_img_rotate"}}' src='../../images/sanjiao.png'></image> -->
      </view>
      <view class='guantong_option_box' style='height:{{MODEL_S_shows?(MODEL_S.length>5?300:MODEL_S.length*60):0}}rpx;'>
        <text class='guantong_option' style='{{MODEL_S_index==MODEL_S.length-1&&"border:0;"}}' wx:for='{{MODEL_S}}' wx:key='this' data-modelsindex='{{index}}' catchtap='model_s_optionTaps'>{{item}}</text>
      </view>
    </view>
</view>
<!--梯型 结束-->
  <!-- 贯通 -->
  <view class="section_keti_guantong">
<text>贯通:</text>
  <!-- 下拉框 -->
  <view class='guantong_select_box'>
      <view class='guantong_select' catchtap='guantong_selectTaps'>
        <text class='guantong_select_text'>{{guantong_datas[guantong_indexs]}}</text>
        <!-- <image class='words_img' src='../../images/sanjiao.png'></image> -->
         <!-- 下拉框 图片  <image class='select_img {{shows&&"select_img_rotate"}}' src='../../images/sanjiao.png'></image> -->
      </view>
      <view class='guantong_option_box' style='height:{{guantong_shows?(guantong_datas.length>5?300:guantong_datas.length*60):0}}rpx;'>
        <text class='guantong_option' style='{{guantong_indexs==guantong_datas.length-1&&"border:0;"}}' wx:for='{{guantong_datas}}' wx:key='this' data-guantong_index='{{index}}' catchtap='guantong_optionTaps'>{{item}}</text>
      </view>
    </view>
</view>
<!-- 载重 -->
<view  class="section_keti_dl">
<text>载重:</text>
<!-- 下拉框 -->
<view  class='dl_select_box'>
<view  class='dl_select' catchtap="dl_selectTaps">
<text class='dl_select_text' >{{DL[DL_index]}}</text>
</view>
<view class='dl_option_box' style='height:{{DL_shows?(DL.length>5?300:DL.length*60):0}}rpx;' >
