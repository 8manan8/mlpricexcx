</view>
<view class='dl_option_box' style='height:{{F_shows?(F.length>5?300:F.length*60):0}}rpx;'>
<text class='dl_option' style='{{F_index==F.length-1&&"border:0;"}}' wx:for="{{F}}" wx:key="this" data-findex1='{{index}}'  catchtap ='f_optionTaps'>{{item}}</text>
</view>
</view>
</view>
  <!-- 厅门 -->
  <view  class="section_keti_dl">
<text>厅门: </text>
<!-- 下拉框 -->
<view  class='dl_select_box'>
<view  class='dl_select' catchtap="nbld_selectTaps">
<text class='dl_select_text' >{{NBLD[NBLD_index]}}</text>
</view>
<view class='dl_option_box' style='height:{{NBLD_shows?(NBLD.length>5?300:NBLD.length*60):0}}rpx;' >
<text class='dl_option' style='{{NBLD_index==NBLD.length-1&&"border:0;"}}' wx:for="{{NBLD}}" wx:key="this" data-nbldindex1='{{index}}'  catchtap ='nbld_optionTaps'>{{item}}</text>
</view>
</view>
</view>
 
 

